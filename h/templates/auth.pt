<html metal:use-macro="main_template">
  <head metal:fill-slot="head" tal:omit-tag>
  </head>
  <body metal:fill-slot="body" tal:omit-tag>
    <div id="auth" class="content paper ng-cloak">
      <metal:main use-macro="load: h:templates/header.pt" />
      <main ng-view></main>
    </div>
    <script type="text/ng-template" id="auth.html">
      <metal:main use-macro="load: h:templates/auth.html" />
    </script>
  </body>
</html>
